<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.co.zenrin.music.play">

    <application
        android:name=".HondaPlayApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/CustomTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ControllerActivity"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".MusicPlayActivity" />
        <activity android:name=".PlayScreenActivity"
                  android:parentActivityName=".MainActivity"/>
        <activity android:name=".RadioPlayer"></activity>
        
        <activity android:name=".MusicArrangeActivity"
            android:parentActivityName=".PlayScreenActivity"></activity>
    </application>

</manifest>